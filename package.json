{
  "name": "remix-todo",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"pnpm start\"",
    "start": "wrangler pages dev --live-reload --compatibility-date=2023-06-21 ./public",
    "typecheck": "tsc",
    "deploy": "pnpm build && wrangler pages deploy ./public",
    "generate": "drizzle-kit generate:sqlite --schema=./app/schema.ts --out=./migrations"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.0.1",
    "@remix-run/cloudflare-pages": "^2.0.1",
    "@remix-run/css-bundle": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "drizzle-orm": "^0.28.6",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@remix-run/dev": "^2.0.1",
    "@remix-run/eslint-config": "^2.0.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.38.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.0",
    "wrangler": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}